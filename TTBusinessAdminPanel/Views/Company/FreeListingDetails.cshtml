@model CommonService.ViewModels.FreelistingDetailModel;
@{
    ViewData["Title"] = "Freelisting";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="block-header">
    <div class="row">
        <div class="col-lg-12 col-md-6 col-sm-7">
            <h2>Freelisting Details</h2>
            <ul class="breadcrumb">
                <li class="breadcrumb-item">Company</li>
                <li class="breadcrumb-item"><a href="@Url.Action("FreeListing", "Company")">Freelisting</a></li>
                <li class="breadcrumb-item"><a href="@Url.Action("Brand", "Company")">FreeLising Details</a></li>
            </ul>
        </div>
    </div>
</div>
<div class="row clearfix">
    <div class="col-lg-12">
        <div class="card">
            <div class="body">
                <table class="table">
                    <tr>
                        <td>Company Name</td>
                        <td>@Model.CompanyName</td>
                        <td> </td>
                    </tr>
                    <tr>
                        <td>Category Name</td>
                        <td>@Model.CategoryName</td>
                        <td> </td>
                    </tr>

                    <tr>
                        <td>Product</td>
                        <td>@Model.RelatedProduct</td>
                        <td></td>
                        </tr><tr>
                        <td>Service</td>
                        <td>@Model.RelatedService</td>
                        <td> </td>
                    </tr>
                    <tr>
                        <td>Brand</td>
                        <td>@Model.Brand</td>
                        <td> </td>
                    </tr>
                </table>

            </div>
        </div>
    </div>
</div>




@section scripts {
    <script>
        $(document).ready(function () {

        });

        function BindDetail(fid) {
            $.ajax({
                type: 'GET',
                url: '@Url.Action("GetAllFreeListingDetails","Company")',
                dataType: 'json',
                data: { id: fid },
                success: function (data) {
                    console.log(data);

                },
                error: function (ex) {

                }
            });
        }


    </script>
}

